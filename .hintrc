{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "link[referrerpolicy]",
          "html[translate]",
          "script[referrerpolicy]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "min-width: fit-content",
          "scroll-behavior",
          "backdrop-filter",
          "scrollbar-width",
          "scrollbar-color"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "meta-viewport": "off"
  }
}